{
    "version": "0.2.0",
    "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "Launch ZWW Extractor",
        "program": "${workspaceFolder}/src/WindWaker/tools/zww_extractor.ts",
        "cwd": "${fileDirname}",
        "outFiles": ["${fileDirname}/**/*.js"],
        "skipFiles": [
            "<node_internals>/**"
        ]
    },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}/src/"
        }
    ]
}
